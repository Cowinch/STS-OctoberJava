package com.cole.ZooKeeper.models;

public class Bat extends Mammal {
	protected double energyLevel=300;

	public Bat() {
		// TODO Auto-generated constructor stub
	}
	
	public void fly() {
		this.energyLevel-=50;
		System.out.println("Whoosh! The bat has taken off! energy reduced by 50" + this.energyLevel);
	}
	
	public void eatHumans() {
		this.energyLevel-=25;
		System.out.println("MUNCH MUNCH! energy increased by 25" + this.energyLevel);
	}
	
	public void attackTown() {
		
	}

}
